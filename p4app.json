{
  "p4_src": "telemetry.p4",
  "switch": "simple_switch",
  "compiler": "p4c",
  "opts": "--target bmv2 --arch v1model --std p4-16",
  "switch_cli": "simple_switch_CLI",
  "cli": true,
  "pcap_dump": true,
  "enable_log": true,
  "topology": {
    "assignment_strategy": "l3",
    "links": [
      ["h1", "s1"],
      ["h2", "s1", {"bw": 0.5, "delay": "50ms"}]
    ],
    "hosts": {
      "h1": {},
      "h2": {}
    },
    "switches": {
      "s1": {"cli_input": "s1-commands.txt"}
    }
  }
}
